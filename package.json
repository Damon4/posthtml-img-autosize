{
  "name": "posthtml-img-autosize",
  "version": "0.1.1",
  "description": "Auto setting width and height of img tag",
  "author": "Kirill Maltsev <maltsevkirill@gmail.com>",
  "license": "MIT",
  "scripts": {
    "compile": "rm -f lib/*.js && node_modules/.bin/babel -d lib/ lib/",
    "lint": "node_modules/.bin/eslint *.js lib/*.es6 test/",
    "test": "npm run lint && npm run compile",
    "posttest": "node_modules/.bin/_mocha --compilers js:babel-core/register --check-leaks",
    "prepublish": "npm run compile"
  },
  "keywords": [
    "posthtml",
    "posthtml-plugin",
    "html",
    "postproccessor",
    "autosize",
    "img",
    "image"
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "bluebird": "^3.1.1",
    "image-size": "^0.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^6.0.0",
    "babel-preset-es2015": "^6.3.13",
    "eslint": "^2.0.0",
    "expect": "^1.13.4",
    "mocha": "^3.0.0",
    "posthtml": "^0.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/posthtml/posthtml-img-autosize.git"
  },
  "bugs": {
    "url": "https://github.com/posthtml/posthtml-img-autosize/issues"
  },
  "homepage": "https://github.com/posthtml/posthtml-img-autosize"
}
